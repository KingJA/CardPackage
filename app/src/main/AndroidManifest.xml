<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kingja.cardpackage">

    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:name=".base.App"
        android:screenOrientation="portrait"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.MainActivity"/>
        <activity android:name=".activity.RentActivity"/>
        <activity android:name=".activity.HouseActivity"/>
        <activity android:name=".activity.ShopActivity"/>
        <activity android:name=".activity.DetailHouseActivity"/>
        <activity android:name=".activity.DetailRentActivity"/>
        <activity android:name=".activity.DetailShopActivity"/>
        <activity android:name=".activity.RoomListActivity"/>
        <activity android:name=".activity.PersonManagerActivity"/>
    </application>



</manifest>